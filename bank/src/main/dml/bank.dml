package pt.ulisboa.tecnico.softeng.bank.domain;

class Bank {
}

class Operation{
	String reference;
	int value;
	DateTime Time;
}

class Client{
	String name;
	String ID;
}

relation DomainRootHasBankApplication {
	.pt.ist.fenixframework.DomainRoot playsRole root {
		multiplicity 1..1;
	}
	Bank playsRole bank {
		multiplicity 0..*;
	}
}

relation BankHasOperation{
	Bank playsRole bank{
		multiplicity 1..1;	
	}
	Operation playsRole operation{
		multiplicity 0..*;
	}

relation BankHasClient{
	Bank playsRole bank{
		multiplicity 1..1;	
	}
	Client playsRole client{
		multiplicity 0..*;
	}

}